      "prop-types": "^15.7.2",
      "sinon": "^7.3.1"
   },
   "dependencies": {
      "immutability-helper": "^3.0.0",
      "lodash": "^4.17.15",
      "react": "^16.8.6",
      "react-dom": "^16.8.6"
   },
   "babel": {
      "presets": [
         "@babel/env",
         "@babel/react"
      ],
      "plugins": [
         "@babel/plugin-proposal-object-rest-spread",
         "@babel/plugin-transform-destructuring",
         "@babel/plugin-proposal-class-properties",
         [
            "@babel/plugin-proposal-decorators",
            {
               "legacy": true
            }
         ]
      ]
